<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="950.0" style="-fx-background-color: #1a1a2e;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.visiboard.pc.controller.AnalyticsController">
   <children>
      <BarChart fx:id="engagementChart" layoutX="50.0" layoutY="50.0" prefHeight="700.0" prefWidth="850.0" title="Weekly Engagement" style="-fx-text-fill: white;">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxis" tickLabelFill="WHITE" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" fx:id="yAxis" tickLabelFill="WHITE" />
        </yAxis>
      </BarChart>
   </children>
</AnchorPane>
